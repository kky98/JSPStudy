<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
	  <!-- Oracle 테이블의 컬럼이 mem_id 이고, DTO의 필드변수가 memId 일때 매칭이 가능하게 하는 옵션 -->
	  <setting name="mapUnderscoreToCamelCase" value="true"/> 
	</settings>
	<!-- mapper vo 매핑 -->
	<typeAliases>
		<typeAlias type="com.future.my.member.vo.MemberVO" alias="MemberVO"></typeAlias>
		<typeAlias type="com.future.my.board.vo.BoardVO" alias="BoardVO"></typeAlias>
		<typeAlias type="com.future.my.free.vo.FreeBoardVO" alias="FreeBoardVO"></typeAlias>
		<typeAlias type="com.future.my.free.vo.FreeBoardSearchVO" alias="FreeBoardSearchVO"></typeAlias>
	</typeAliases>
	
</configuration>